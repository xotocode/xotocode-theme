    { "scope": "variable.parameter.function", "settings": { "foreground": "--xotocode-light-orange-0" } },
    { "scope": [ "entity.name.type", "entity.other.inherited-class", "meta.function-call", "meta.instance.constructor", "entity.other.attribute-name", "entity.name.function", "constant.keyword.clojure" ], "settings": { "foreground": "--xotocode-light-purple-0" } },
    { "scope": [ "entity.name.tag", "string.quoted", "string.regexp", "string.interpolated", "string.template", "keyword.other.template" ], "settings": { "foreground": "--xotocode-light-green-0" } },
    { "scope": "token.info-token", "settings": { "foreground": "--xotocode-light-blue-2" } },
    { "scope": "token.warn-token", "settings": { "foreground": "--xotocode-light-orange-2" } },
    { "scope": [ "token.error-token", "invalid" ], "settings": { "foreground": "--xotocode-light-red-2" } },
    { "scope": "token.debug-token", "settings": { "foreground": "--xotocode-light-purple-2" } },
    { "scope": "markup.inserted", "settings": { "foreground": "--xotocode-light-green-0" } },
    { "scope": "markup.deleted", "settings": { "foreground": "--xotocode-light-red-0" } },
    { "scope": "markup.changed", "settings": { "foreground": "--xotocode-light-bright-blue-1" } },
    { "scope": "meta.diff.header", "settings": { "foreground": "--xotocode-light-medium-gray-0" } },
    { "scope": [ "constant.numeric", "constant.language", "variable.language.this", "variable.other.class", "variable.other.constant", "meta.property-name", "meta.property-value-0", "support" ], "settings": { "foreground": "--xotocode-light-bright-blue-1" } },
    { "scope": [ "constant.numeric.css", "support.function.misc.css", "constant.other.color.rgb-value.hex.css", "punctuation.section.function.begin.bracket.round.css", "punctuation.section.function.end.bracket.round.css" ], "settings": { "foreground": "--xotocode-light-dark-gray-2" } },

    { "name": "comparison and logical", "scope": ["keyword.operator.comparison", "keyword.operator.logical"], "settings": { "foreground": "--xotocode-light-pale-orange-0" } },
    { "name": "variable property", "scope": ["variable.other.property" ], "settings": { "foreground": "--xotocode-light-gray-5"  } },
    { "name": "variable property", "scope": ["support.variable.property"], "settings": { "foreground": "--xotocode-light-blue-4"  } },
    { "name": "variable object", "scope": ["variable.other.object", ""], "settings": { "foreground": "--xotocode-light-gray-2" } },
