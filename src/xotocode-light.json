    { "scope": [ "keyword.operator.accessor", "meta.group.braces.round.function.arguments", "meta.template.expression", "meta.interpolation", "markup.fenced_code meta.embedded.block" ], "settings": { "foreground": "--xotocode-light-black-0" } },
